{
  "compileOnSave": true,
  "compilerOptions": {
    "target": "es6",
    "sourceMap": true,
    "noImplicitAny": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "outDir": "bin",
    "module": "commonjs",
    "strictNullChecks": true,
    "noImplicitThis": true,
    "noUnusedParameters": false,
    "noUnusedLocals": false
  }
}
